<link rel="/logo/apple-touch-icon" href="/static/apple-touch-icon.png"/>
<link rel="icon" href="/static/logo/favicon-32.png" type="image/png"/>
<link rel="icon" href="/static/logo/favicon-16.png" type="image/pg"/>

<link rel="stylesheet" href="/static/css/base.css" type="text/css"/>
<link rel="stylesheet" href="https://fonts.googleapis.com/css?family=Open+Sans"/>
